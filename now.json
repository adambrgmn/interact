{
  "version": 2,
  "builds": [
    {
      "src": "site/package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(?<rest>.*)",
      "dest": "/site/$rest",
      "headers": { "cache-control": "public, max-age=31536000" }
    }
  ],
  "regions": ["bru"],
  "alias": ["interact"]
}
